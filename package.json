{
  "name": "vrc-scraper-data",
  "version": "0.0.3",
  "description": "A Scraper & API Website for VRChat to retrieve information directly from the site & the API to obtain information on a user (friend) such as groups joined, group represented, profile, rank, etc...",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts:build": "tsc",
    "ts:clean": "tsc --build --clean",
    "ts:cb": "npm run ts:clean && npm run ts:build",
    "//": "SRC commands for development",
    "scrape:auth": "ts-node src/services/puppeteer_scraper/auth_data.ts",
    "scrape:user": "ts-node src/services/puppeteer_scraper/user_data.ts",
    "web:api": "ts-node src/services/express_web/server.ts",
    "//": "DIST commands for production",
    "dist/scrape:auth": "node dist/services/puppeteer_scraper/auth_data.js",
    "dist/scrape:user": "node dist/services/puppeteer_scraper/user_data.js",
    "dist/web:api": "node dist/services/express_web/server.js",
    "//": "Run commands",
    "prestart": "npm run ts:cb",
    "start": "concurrently -i -p \"[{name}] {time}-{pid}\" -n \"SCRP,HTTP\" -c \"bgRed.bold,bgBlue.bold\" \"npm run dist/scrape:user\" \"npm run dist/web:api\" --default-input-target"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kyuddle/VRCScraperUserData.git"
  },
  "keywords": [
    "scrape",
    "scraper",
    "user",
    "data",
    "userdata",
    "vrc",
    "vrchat",
    "api",
    "website",
    "profile",
    "friends",
    "group",
    "puppeteer",
    "express"
  ],
  "author": "Kyuddle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kyuddle/VRCScraperUserData/issues"
  },
  "homepage": "https://github.com/Kyuddle/VRCScraperUserData#readme",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/puppeteer": "^7.0.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "puppeteer": "^23.1.1",
    "readline": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
