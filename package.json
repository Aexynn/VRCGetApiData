{
  "name": "vrc-scraper-data",
  "version": "0.0.2",
  "description": "A Scraper & API Website for VRChat to retrieve information directly from the site & the API to obtain information on a user (friend) such as groups joined, group represented, profile, rank, etc...",
  "main": "launch.js",
  "scripts": {
    "scrape:user": "node services/puppeteer_scraper/user_data.js",
    "web:start": "node services/express_web/server.js",
    "all:launch": "concurrently -p \"[{name}] {time}-{pid}\" -n \"SCRPE,HTTP\" -c \"bgRed.bold,bgBlue.bold\" \"node run scrape:user\" \"node run web:start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kyuddle/VRCScraperUserData.git"
  },
  "keywords": [
    "scrape",
    "scraper",
    "user",
    "data",
    "userdata",
    "vrc",
    "vrchat",
    "api",
    "website",
    "profile",
    "friends",
    "group",
    "puppeteer",
    "express"
  ],
  "author": "Kyuddle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kyuddle/VRCScraperUserData/issues"
  },
  "homepage": "https://github.com/Kyuddle/VRCScraperUserData#readme",
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "puppeteer": "^23.1.0",
    "readline": "^1.3.0"
  }
}
