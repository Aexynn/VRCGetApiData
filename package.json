{
  "name": "vrc-get-api-data",
  "version": "0.0.5",
  "description": "A Scraper/API user & API Website for VRChat to retrieve information directly from the site & the API to obtain information on a user (friend) such as groups joined, group represented, profile, rank, etc...",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts:build": "tsc",
    "ts:clean": "tsc --build --clean",
    "ts:cb": "npm run ts:clean && npm run ts:build",
    "//": "Run commands",
    "scrape:auth": "ts-node src/services/puppeteer_scraper/initiate_auth.ts",
    "//api:auth": "ts-node src/services/axios_fetch/initiate_auth.ts",
    "//api:auth:force": "npm run api:auth -- --force",
    "//api:auth:check": "npm run api:auth -- --check",
    "web:api": "ts-node src/services/express_web/server.ts",
    "api:user": "ts-node src/services/axios_fetch/users/get_user_infos.ts",
    "api:user:groups": "ts-node src/services/axios_fetch/users/get_user_groups.ts",
    "api:group": "ts-node src/services/axios_fetch/groups/get_group_data.ts",
    "api:all": "ts-node src/services/axios_fetch/get_all_data.ts",
    "dist/scrape:auth": "node src/services/puppeteer_scraper/initiate_auth.js",
    "//dist/api:auth": "node dist/services/axios_fetch/initiate_auth.js",
    "//dist/api:auth:force": "npm run dist/api:auth -- --force",
    "//dist/api:auth:check": "npm run dist/api:auth -- --check",
    "dist/web:api": "node dist/services/express_web/server.js",
    "dist/api:user": "node dist/services/axios_fetch/users/get_user_infos.js",
    "dist/api:user:groups": "node dist/services/axios_fetch/users/get_user_groups.js",
    "dist/api:group": "node dist/services/axios_fetch/groups/get_group_data.js",
    "dist/api:all": "node dist/services/axios_fetch/get_all_data.js",
    "prestart": "npm run ts:cb",
    "start": "concurrently -i -p \"[{name}] {time}-{pid}\" -n \"AXIOS,WBAPI\" -c \"bgRed.bold,bgBlue.bold\" \"npm run dist/api:all\" \"npm run dist/web:api\" --default-input-target"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sinoryn/VRCGetApiData.git"
  },
  "keywords": [
    "scrape",
    "scraper",
    "user",
    "data",
    "userdata",
    "vrc",
    "vrchat",
    "api",
    "website",
    "profile",
    "friends",
    "group",
    "puppeteer",
    "express",
    "axios"
  ],
  "author": "Sinoryn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Sinoryn/VRCGetApiData/issues"
  },
  "homepage": "https://github.com/Sinoryn/VRCGetApiData#readme",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/puppeteer": "^7.0.4",
    "axios": "^1.7.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "js-base64": "^3.7.7",
    "path": "^0.12.7",
    "puppeteer": "^23.1.1",
    "readline": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
